package com.mario90900.fruitsofnature.item;

import com.mario90900.fruitsofnature.reference.UnlocalizedNames;

import net.minecraft.block.Block;
import net.minecraft.item.ItemStack;
//import net.minecraft.world.IBlockAccess;
import net.minecraftforge.common.EnumPlantType;

public class ItemWheatSeeds extends ItemGroundSeeds{
	public ItemWheatSeeds(Block plant){
		super(plant);
		this.setUnlocalizedName(UnlocalizedNames.WHEAT_SEEDS);
	}
	
	public ItemWheatSeeds(Block plant, ItemStack stack, int potency, int yield, int growth){
		this(plant);
		this.setPotency(stack, potency);
		this.setYield(stack, yield);
		this.setGrowth(stack, growth);
	}
}
