package com.mario90900.fruitsofnature.tileentity;

import java.util.Random;

import com.mario90900.fruitsofnature.reference.AveragePlantStats;

import net.minecraft.world.World;

public class TileEntityWheatPlant extends TileGroundPlant{
	public void onBlockTick(World world, int x, int y, int z, Random rand){
		super.onBlockTick(world, x, y, z, rand, AveragePlantStats.WHEAT_POTENCY, AveragePlantStats.WHEAT_YIELD, AveragePlantStats.WHEAT_GROWTH);
	}
}
